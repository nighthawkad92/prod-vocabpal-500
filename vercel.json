{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "installCommand": "npm --prefix web install",
  "buildCommand": "npm --prefix web run build",
  "outputDirectory": "web/dist",
  "rewrites": [
    {
      "source": "/designsystem",
      "destination": "/index.html"
    },
    {
      "source": "/designsystem/(.*)",
      "destination": "/index.html"
    }
  ]
}
